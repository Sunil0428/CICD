pipeline{
    agent{
        label 'Agent-1'
    }
    environment {
        mysql_host = 'mysql.mydevops.tech' // Replace with your EC2 public DNS or IP
    }
    stages{
        stage('login into mysql ec2'){
            steps{
                script{
                    sshagent(['mysql-key']) { // Replace with the ID of your SSH credentials
                        sh '''
                            ssh -o StrictHostKeyChecking=no ec2-user@$mysql_host <<EOF
                            echo "Successfully logged in to EC2 instance"
                        '''
                    }
                }
            }
        }
     
    }
}